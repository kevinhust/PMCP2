# terraform.tfvars

# Alert Mailbox
alert_email = "xxxx@myseneca.ca" #put your Mailbox

# AWS Region
aws_region = "us-east-1"

# Kinesis Stream Name
kinesis_stream_name = "stock-stream"

# DynamoDB Table Name
dynamodb_table_name = "stock-indicators"

# S3 Bucket Name
s3_bucket_name = "kevinw-p2" 

# Alpha Vantage API Key
alpha_vantage_api_key = "YOUR_ALPHA_VANTAGE_API_KEY" #put your key here!

# Stock Symbols
stock_symbols = ["AAPL", "MSFT", "GOOGL", "AMZN", "TSLA"] #you can change to your one
